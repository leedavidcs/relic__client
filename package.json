{
	"name": "client",
	"description": "Stock-Tracker App",
	"version": "0.1.0",
	"private": true,
	"author": {
		"name": "David Lee",
		"email": "lee.david.cs@gmail.com"
	},
	"scripts": {
		"start": "react-app-rewired start",
		"build": "react-app-rewired build",
		"audit-ci": "audit-ci --moderate --report-type full --package-manager npm",
		"format": "prettier ./**/*.{json,js,ts,jsx,tsx,gql,graphql} -l",
		"format:fix": "npm run format -- --write",
		"generate": "graphql-codegen",
		"generate:all": "npm-run-all generate generate:json generate:inputs",
		"generate:json": "npm run with-script -- scripts/generate-graphql-schema-json",
		"generate:inputs": "npm run with-script -- scripts/generate-graphql-file-inputs",
		"lint": "eslint ./src/**/*.{ts,tsx}",
		"lint:fix": "npm run lint -- --fix",
		"storybook": "start-storybook -p 6006",
		"storybook:build": "build-storybook",
		"test": "jest",
		"test:coverage": "npm test -- --coverage",
		"test:sonar": "npm run with-script -- scripts/generate-sonar && npm test -- --coverage --testResultsProcessor=jest-sonar-reporter",
		"with-script": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
